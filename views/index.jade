html
  head
    title Berichtshefte
    meta(charset='UTF-8')
    link(rel='stylesheet' href='/main.css')
  body
    h1 Berichtshefte

    each reports, yearNo in years
      div(class='year')
        h2= yearNo
        ul(class='year-list')
          each report in reports
            li(class=typeof report.filed === 'undefined' ? '' : 'filed')
              a(href=report.url target='_blank')= report.monthName

