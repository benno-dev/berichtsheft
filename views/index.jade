html
  head
    title Berichtshefte
    meta(charset='UTF-8')
    link(rel='stylesheet' href='/main.css')
  body
    h1 Berichtshefte

    each reports, yearNo in years
      h2= yearNo
      ul
        each report in reports
          li(class=typeof report.filed === 'undefined' ? '' : 'filed')
            a(href=report.url)= report.monthName

